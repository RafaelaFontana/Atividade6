programa {
	funcao inicio() {
	    
		/*5. Faça um programa, utilizando “para”, que:
        a. Peça para o usuário inserir um número N;
        b. Escreva na tela todos os números de 1 até N
        e pule 3 linhas; */
        
        inteiro n = 0
        
        escreva("Fale um valor para ser agregado a n: ")
        leia(n)
        
        
        para(inteiro c = 1; c<=n; c++){
            escreva(c + "\n\n\n")
        }
        
        
	}
}
