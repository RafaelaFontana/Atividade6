programa 
{
	funcao inicio() 
	{
	    
		/* 1. Faça um programa que imprima na tela 
		os números de 1 a 20, um abaixo do outro.”
        a. Use a estrutura de repetição “enquanto”; */
        
        inteiro numero = 1
    
        enquanto(numero<= 20){
            escreva(numero, "\n")
            numero = numero + 1
            
        }
	}
}
